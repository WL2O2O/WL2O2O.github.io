#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/CS_GUIDER/hexo/node_modules/.store/fast-xml-parser@4.3.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/CS_GUIDER/hexo/node_modules/.store/fast-xml-parser@4.3.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../fast-xml-parser@4.3.2/node_modules/fast-xml-parser/src/cli/cli.js" "$@"
else
  exec node  "$basedir/../../../../../fast-xml-parser@4.3.2/node_modules/fast-xml-parser/src/cli/cli.js" "$@"
fi
